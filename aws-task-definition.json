{
    "networkMode": "bridge",
    "taskRoleArn": "arn:aws:iam::908660671515:role/ecsTaskRole-ecs-deployment",
    "containerDefinitions": [
        {
            "volumesFrom": [],
            "memory": 512,
            "portMappings": [],
            "essential": true,
            "entryPoint": [],
            "mountPoints": [],
            "name": "ecs-deployment",
            "environment": [
                {
                    "name": "ECS_TIMEOUT",
                    "value": "300"
                },
                {
                    "name": "SLACK_CHANNEL",
                    "value": "<SLACK_CHANNEL>"
                },
                {
                    "name": "ECS_CLUSTER",
                    "value": "default"
                },
                {
                    "name": "SLACK_WEBHOOK",
                    "value": "<SLACK_WEBHOOK>"
                },
                {
                    "name": "SQS_URL",
                    "value": "<SQS_URL>"
                }
            ],
            "links": [],
            "image": "uqlibrary/docker-ecs-deployment:latest",
            "command": [],
            "logConfiguration": {
                "logDriver": "syslog",
                "options": {
                    "syslog-address": "<SYSLOG_ENDPOINT>",
                    "tag": "{{.Name}}/{{.ID}}"
                }
            },
            "cpu": 128,
            "memoryReservation": 256
        }
    ],
    "placementConstraints": [],
    "volumes": [],
    "family": "ecs-deployment"
}
